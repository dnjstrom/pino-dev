{
  "name": "pino-dev",
  "version": "1.0.6",
  "description": "A simple prettifier for pino with built-in support for commonly used ecosystem packages.",
  "author": "Daniel Str√∂m <D@nielstrom.se>",
  "license": "ISC",
  "main": "dist/index.js",
  "bin": {
    "pino-dev": "dist/bin.js"
  },
  "scripts": {
    "dev": "clear; ts-node-dev --inspect --clear --rs test/utils/dev.ts",
    "pretest": "sh ./test/utils/generateAllExamples.sh",
    "test": "npm run test:types && npm run lint && npm run test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "node --inspect ./node_modules/.bin/jest --runInBand",
    "test:unit:watch": "node --inspect ./node_modules/.bin/jest --runInBand --watch",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint '*/**/*.ts' --max-warnings 0 --fix",
    "build:release": "npm test && NODE_ENV=production npm run build && npm publish"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ]
  },
  "prettier": {},
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/*.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dnjstrom/pino-dev"
  },
  "bugs": {
    "url": "https://github.com/dnjstrom/pino-dev/issues"
  },
  "homepage": "https://github.com/dnjstrom/pino-dev#readme",
  "keywords": [
    "pino",
    "pretty-printer",
    "ndjson",
    "logger",
    "transport"
  ],
  "devDependencies": {
    "@types/args": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/pino": "6.3.4",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^2.3.13",
    "@types/split2": "^3.2.1",
    "@types/strip-ansi": "5.2.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "mockdate": "^3.0.5",
    "pino": "6.8.0",
    "pino-debug": "2.0.0",
    "pino-http": "6.6.0",
    "prettier": "^2.5.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "type-fest": "^2.11.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@hapi/bourne": "2.0.0",
    "args": "5.0.1",
    "chalk": "^4.1.2",
    "date-fns": "^2.28.0",
    "debug": "^4.3.3",
    "joycon": "^3.1.1",
    "pump": "^3.0.0",
    "readable-stream": "^3.6.0",
    "split2": "^4.1.0",
    "strip-ansi": "^6.0.1"
  }
}
